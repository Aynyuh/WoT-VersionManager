<Page x:Class="VersionManagerUI.MessageWindows.Buttons.ButtonsOkCancel"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:VersionManagerUI.MessageWindows.Buttons"
      mc:Ignorable="d" 
      d:DesignHeight="40" d:DesignWidth="350"
      Title="ButtonsOkCancel">
    
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"></ColumnDefinition>
            <ColumnDefinition Width="1*"></ColumnDefinition>
            <ColumnDefinition Width="1*"></ColumnDefinition>
            <ColumnDefinition Width="1*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnOk" Grid.Column="2" Margin="5,5" Click="btnOk_Click">OK</Button>
        <Button x:Name="btnCancel" Grid.Column="3" Margin="5,5" Click="btnCancel_Click">Cancel</Button>
    </Grid>
    <d:DesignerProperties.DesignStyle>
        <Style TargetType="Page">
            <Setter Property="Background" Value="White"></Setter>
        </Style>
    </d:DesignerProperties.DesignStyle>
</Page>
